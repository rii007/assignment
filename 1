#include <iostream>

int main() {
    int i,n,a[100],min,max;
    
   scanf("%d",&n);
   for(i=0;i<n;i++){
       scanf("%d",&a[i]);
   }
   min=max=a[0];
   for(i=1;i<n;i++){
       if(a[i]>max){
           max=a[i];
       }
       if(min>a[i]){
           min=a[i];
       }
   }
  printf("%d %d",min,max);

    return 0;
}
