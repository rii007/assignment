#include <stdio.h>

int main()
{
    int n,i,j,k;
    printf("enter n:-");
    scanf("%d",&n);
    for(i=1;i<=n;i++){
        for(k=1; k<=n-i; k++)
        {
            printf(" ");
        }
        for(j=1;j<=n;j++){
            if(i==1||i==n||j==n||j==1){
                printf("*");
            }
            else{
                printf(" ");
            }
        }
        printf("\n");
    }

    return 0;
}



